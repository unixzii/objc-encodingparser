#ifndef __OBJC_MODIFIER
#define __OBJC_MODIFIER(name, value, rep)
#endif

//              name                       val        rep
__OBJC_MODIFIER(OBJC_MODIFIER_COMPLEX,     (1 << 0),  'j')
__OBJC_MODIFIER(OBJC_MODIFIER_ATOMIC,      (1 << 2),  'A')
__OBJC_MODIFIER(OBJC_MODIFIER_CONST,       (1 << 3),  'r')
__OBJC_MODIFIER(OBJC_MODIFIER_IN,          (1 << 4),  'n')
__OBJC_MODIFIER(OBJC_MODIFIER_INOUT,       (1 << 5),  'N')
__OBJC_MODIFIER(OBJC_MODIFIER_OUT,         (1 << 6),  'o')
__OBJC_MODIFIER(OBJC_MODIFIER_BYCOPY,      (1 << 7),  'O')
__OBJC_MODIFIER(OBJC_MODIFIER_BYREF,       (1 << 8),  'R')
__OBJC_MODIFIER(OBJC_MODIFIER_ONEWAY,      (1 << 9),  'V')
__OBJC_MODIFIER(OBJC_MODIFIER_GNUREGISTER, (1 << 10), '+')
